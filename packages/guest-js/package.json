{
  "name": "tauri-plugin-pinia",
  "version": "0.6.0",
  "description": "Persistent Pinia stores for Tauri",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@9.10.0",
  "homepage": "https://tb.dev.br/tauri-plugin-pinia",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/tauri-plugin-pinia.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/tauri-plugin-pinia/issues"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "vue",
    "tauri",
    "pinia",
    "plugin"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "typedoc": "typedoc --plugin typedoc-plugin-mdn-links"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0-rc.4",
    "@tb-dev/utils": "^3.3.0",
    "@vueuse/core": "^11.1.0",
    "pinia": "^2.2.2",
    "vue": "^3.5.6"
  },
  "devDependencies": {
    "typedoc": "^0.26.7",
    "typedoc-plugin-mdn-links": "^3.2.12"
  }
}